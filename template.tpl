___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
log('data =', data);

const createArgumentsQueue = require('createArgumentsQueue');
const gtag = createArgumentsQueue('gtag', 'dataLayer');

const makeTableMap = require('makeTableMap');
const getTimestampMillis = require ('getTimestampMillis');

// fields_to_set
let fields_to_set;
if(data.fields_to_set) {
  fields_to_set = makeTableMap(data.fields_to_set, 'field_name', 'value');
}
else {
  fields_to_set = {};
}
fields_to_set.send_to = data.measurement_id;


if(data.user_id) {
  fields_to_set.user_id = data.user_id_value;
}

if(data.hit_timestamp) {
  fields_to_set.td_hit_timestamp = getTimestampMillis()*1000;
}

// user_properties
let user_properties;
if(data.user_properties) {
  user_properties = makeTableMap(data.user_properties, 'field_name', 'value');
}
else {
  user_properties = {};
}

if(data.crm_id) {
  user_properties.crm_id = data.user_id_value;
}

gtag('get', data.measurement_id, 'client_id', function(client_id) {
  if(data.client_id) {
    user_properties.td_client_id = '(' + client_id.toString() + ')';
  }
  
  gtag('set', 'user_properties', user_properties);
  gtag('event', data.event_name, fields_to_set);
});

data.gtmOnSuccess();


